{
  "model": {
    "type": "simple",
    "hidden_dims": [64, 32],
    "dropout_rate": 0.2,
    "use_batch_norm": false,
    "use_residual": false,
    "learning_rate": 0.001,
    "weight_decay": 0.0001
  },
  "data": {
    "batch_size": 32,
    "test_size": 0.2,
    "random_state": 42,
    "normalize": true
  },
  "federated_learning": {
    "num_rounds": 3,
    "local_epochs": 1,
    "min_fit_clients": 2,
    "min_eval_clients": 2,
    "min_available_clients": 2,
    "strategy": "FedAvg"
  },
  "system": {
    "device": "auto",
    "log_level": "INFO",
    "seed": 42,
    "output_dir": "outputs"
  }
}
